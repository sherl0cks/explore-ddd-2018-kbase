      <bpmn2:script><![CDATA[System.out.println(data);

long parentProcessId = kcontext.getProcessInstance().getParentProcessInstanceId();
if ( Map.class.isAssignableFrom(data.getClass())){
    for ( String key : data.keySet()){
        kcontext.getKieRuntime().getProcessInstance(parentProcessId).setVariable(key, data);
    }
} else {
    kcontext.getProcessInstance().setVariable(name, data);
}]]></bpmn2:script>
